<main class="main">
  <div class = inner>
  <%= search_form_for @t, url: teams_search_path do |f| %>
  <%= f.label :name_eq, '所属の市町村' %>
  <%= f.collection_select :city_id, City.all, :id, :name,  include_blank: '指定なし' %>
  <%= f.submit '検索' %>
<% end %>
    <div class="card__wrapper">
      <%= render partial: 'team', collection: @teams %>
    </div>
  </div>
</main>
